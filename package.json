{
  "name": "linter-lua",
  "main": "./lib/main",
  "version": "1.0.4",
  "description": "Lint Lua on the fly, using luac -p or luajit -bl",
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-lua"
  },
  "license": "MIT",
  "dependencies": {
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^5.0.0"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticCommits": true,
    "rangeStrategy": "pin",
    "packageRules": [
      {
        "packagePatterns": [
          "^eslint"
        ],
        "groupName": "ESLint packages"
      }
    ]
  }
}
